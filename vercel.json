{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "http://localhost:3000/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}